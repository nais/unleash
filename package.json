{
  "name": "nais-unleash",
  "version": "4.0.0",
  "description": "NAIS customized Unleash feature flag server - monorepo",
  "private": true,
  "scripts": {
    "build:all": "pnpm -r build",
    "build:shared": "pnpm --filter @nais/unleash-shared build",
    "build:v5": "pnpm --filter unleash-v5 build",
    "build:v6": "pnpm --filter unleash-v6 build",
    "build:v7": "pnpm --filter unleash-v7 build",
    "test:all": "pnpm -r test",
    "test:shared": "pnpm --filter @nais/unleash-shared test",
    "test:v5": "pnpm --filter unleash-v5 test",
    "test:v6": "pnpm --filter unleash-v6 test",
    "test:v7": "pnpm --filter unleash-v7 test",
    "lint:all": "pnpm -r lint",
    "lint:shared": "pnpm --filter @nais/unleash-shared lint",
    "lint:v5": "pnpm --filter unleash-v5 lint",
    "lint:v6": "pnpm --filter unleash-v6 lint",
    "lint:v7": "pnpm --filter unleash-v7 lint"
  },
  "author": "NAIS <aura@nav.no>",
  "license": "MIT",
  "engines": {
    "node": "^24.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/express-session": "1.18.0",
    "@types/faker": "6.6.9",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/make-fetch-happen": "10.0.4",
    "@types/node": "20.14.10",
    "@types/semver": "7.5.8",
    "@types/stoppable": "1.1.3",
    "@types/supertest": "6.0.2",
    "@types/type-is": "1.6.6",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "eslint": "^9.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "5.5.3",
    "typescript-eslint": "^7.16.0"
  }
}
